  volume={},
  number={},
  pages={1733-1736},
  abstract={This article mainly demonstrates the relationship between multimedia network teaching and innovation capability cultivation. Multimedia network teaching, which breaks the limitation of space and time to traditional education brings impacts to traditional teaching methods. With features of integration, interaction, autonomy, etc, it cultivated students' innovation consciousness, thinking, and capability, and created conditions for cultivating qualified innovation talents to the society.},
  keywords={Multimedia communication;Technological innovation;Educational institutions;Computers;Courseware;Streaming media;Multimedia Network Teaching;Innovation Capability;Teaching Mode},
  doi={10.1109/ICCIS.2013.452},
  ISSN={},
  month={June},}@INPROCEEDINGS{10940750,
  author={Del Pilar Aldana Bravo, Gissela and Caballero, Jesús Emilio Agustín Padilla and De La Cruz, Jhonn Robert Ruiz and De Ruiz, Kathy Flores Cabrera and Purguaya, Ruth Janet Toledo and Cristóbal, Obed Isaías Matías},
  booktitle={2025 International Conference on Pervasive Computational Technologies (ICPCT)}, 
  title={Teaching Machine Learning in K–12 Education: Potential and Challenges}, 
  year={2025},
  volume={},
  number={},
  pages={702-705},
  abstract={A growing number of from kindergarten to twelve and colleges and universities are using machine learning (ML) methods, which have shown promise in a variety of computer domains. The study needs to comprehend how awareness and freedom are formed in these frameworks. However, one of the challenges facing computer education studies is incorporating artificial intelligence and classical mathematical thinking into the school curriculum. Research on how individuals learn to prepare, evaluate, enhance, and implement machine learning systems is lacking, particularly in the K–12 educational area, despite machine learning's pivotal role in today's technology. This article places current research in the computing learning setting. Outlines variations that K–12 computing instructors should consider when tackling this obstacle, and graphs arising pathways in educational reality, concept, and innovation related to instructing machine learning in schools in grades K–12. To properly include ML in the more comprehensive K–12 computer curriculum, the paper focuses on significant aspects of the perspective shift.},
  keywords={Technological innovation;Ethics;Computer aided instruction;Machine learning;Machine Learning (ML);K–12 Education;Artificial Intelligence (AI);Pedagogy;Technology},
  doi={10.1109/ICPCT64145.2025.10940750},
  ISSN={},
  month={Feb},}@INPROCEEDINGS{6463199,
  author={Lin, Xiongwei and Hu, Dabin and Chen, Lei},
  booktitle={2012 IEEE Fifth International Conference on Advanced Computational Intelligence (ICACI)}, 
  title={The application of fuzzy theory in submarine maneuver control}, 
  year={2012},
  volume={},
  number={},
  pages={428-432},
  abstract={In the domain of industry control, PID control is the basic algorithm, which has been used for a long time because of its simple structure, easy realization and many other virtues. For many simple control objects, PID control could easily achieve perfect effect, but for some complex object the effect is not so perfect. The submarine maneuver control system is a nonlinear six degree of freedom model, which has the great inertia variational parameters and many other complex characters. It is difficult for single PID to control the system because of the complex characters. In many instances, the best control is obtained in the manual mode using experienced operators. Fuzzy control translates the thinking of experienced operators into control algorithm, which has proven effective for complex, nonlinear, imprecisely-defined model. In order to advance the control effect, fuzzy theory is introduced and simulation experiments are carried out in this paper. The result shows that fuzzy PID control designed for the unstable nonlinear model of submarine maneuver respond more rapidly and robustly than single PID control.},
  keywords={PD control;Underwater vehicles;Niobium;Fuzzy control;Mathematical model;Equations},
  doi={10.1109/ICACI.2012.6463199},
