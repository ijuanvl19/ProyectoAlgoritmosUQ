  month={Oct},}@ARTICLE{9209148,
  author={Causey, Jason L. and Li, Keyu and Chen, Xianghao and Dong, Wei and Walker, Karl and Qualls, Jake A. and Stubblefield, Jonathan and Moore, Jason H. and Guan, Yuanfang and Huang, Xiuzhen},
  journal={IEEE/ACM Transactions on Computational Biology and Bioinformatics}, 
  title={Spatial Pyramid Pooling With 3D Convolution Improves Lung Cancer Detection}, 
  year={2022},
  volume={19},
  number={2},
  pages={1165-1172},
  abstract={Lung cancer is the leading cause of cancer deaths. Low-dose computed tomography (CT)screening has been shown to significantly reduce lung cancer mortality but suffers from a high false positive rate that leads to unnecessary diagnostic procedures. The development of deep learning techniques has the potential to help improve lung cancer screening technology. Here we present the algorithm, DeepScreener, which can predict a patient's cancer status from a volumetric lung CT scan. DeepScreener is based on our model of Spatial Pyramid Pooling, which ranked 16th of 1972 teams (top 1 percent)in the Data Science Bowl 2017 competition (DSB2017), evaluated with the challenge datasets. Here we test the algorithm with an independent set of 1449 low-dose CT scans of the National Lung Screening Trial (NLST)cohort, and we find that DeepScreener has consistent performance of high accuracy. Furthermore, by combining Spatial Pyramid Pooling and 3D Convolution, it achieves an AUC of 0.892, surpassing the previous state-of-the-art algorithms using only 3D convolution. The advancement of deep learning algorithms can potentially help improve lung cancer detection with low-dose CT scans.},
  keywords={Cancer;Computed tomography;Lung;Biological system modeling;Training;Testing;Machine learning;Lung cancer screening;low-dose CT scan;deep learning algorithm;convolutional neural network (CNN);medical imaging},
  doi={10.1109/TCBB.2020.3027744},
  ISSN={1557-9964},
  month={March},}@INPROCEEDINGS{10788655,
  author={Bardales, Einstein Sánchez and Marín, Yuri Reina and Caro, Omer Cruz and Rituay, Angelica M. Carrasco and Fernández, Milena Torres and Santos, River Chavez},
  booktitle={2024 International Symposium on Accreditation of Engineering and Computing Education (ICACIT)}, 
  title={Key Competencies for Industry 5.0 in Undergraduate Engineering Students}, 
  year={2024},
  volume={},
  number={},
  pages={1-7},
  abstract={Core competencies have a vital role to enable a sustainable change towards Industry 5.0. In view of this, the study aims to characterize the university training skills for adaptation to Industry 5.0 of engineering students at the National University Toribio Rodriguez de Mendoza of Amazonas (UNTRM). It was a descriptive, non-experimental quantitative research; the study population was made up of students and professors of engineering majors, with a sample of 138 students and 28 professors. The results show, that engineering students possess high levels of competencies related to Industry 5.0, including critical thinking (71.7%), creativity (69.6%), digital competencies (61.6%), resilience (60.9%), ethics and collaboration (69.6%). These results are the consequence of innovative pedagogical strategies implemented by teachers, who integrate methodologies that foster advanced thinking, technological adaptability and ethical awareness, thus preparing future engineers for the challenges of a rapidly evolving world.},
  keywords={Training;Ethics;Accreditation;Teamwork;Fifth Industrial Revolution;Engineering students;Sustainable development;Creativity;Standards;Resilience;Critical thinking;problem solving;creativity;innovation;digital skills;ethical responsibility;academic resilience;teamwork},
  doi={10.1109/ICACIT62963.2024.10788655},
  ISSN={2771-6058},
  month={Oct},}@INPROCEEDINGS{4145929,
  author={Coutinho, Eduardo and Gimenes, Marcelo and Martins, Joao M. and Miranda, Eduardo R.},
  booktitle={2005 portuguese conference on artificial intelligence}, 
  title={Computational Musicology: An Artificial Life Approach}, 
  year={2005},
  volume={},
  number={},
  pages={85-93},
  abstract={Artificial life (A-Life) and evolutionary algorithms (EA) provide a variety of new techniques for making and studying music. EA have been used in different musical applications, ranging from new systems for composition and performance, to models for studying musical evolution in artificial societies. This paper starts with a brief introduction to three main fields of application of EA in music, namely sound design, creativity and computational musicology. Then it presents our work in the field of computational musicology. Computational musicology is broadly defined as the study of music with computational modelling and simulation. We are interested in developing A-Life-based models to study the evolution of musical cognition in an artificial society of agents. In this paper we present the main components of a model that we are developing to study the evolution of musical ontogenies, focusing on the evolution of rhythms and emotional systems. The paper concludes by suggesting that A-Life and EA provide a powerful paradigm for computational musicology},
  keywords={Psychoacoustic models;Music;Network synthesis;Signal synthesis;Synthesizers;Chaos;Oscillators;Computational modeling;Rhythm;Application software;Artificial Life;Music;Computational Musicology;Synchronization and Evolution of Rhythm;Musical Ontogensis;Emergence of Emotion},
  doi={10.1109/EPIA.2005.341270},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{9458047,
