  url={https://ieeexplore-ieee-org.crai.referencistas.com/document/8709316},}@INPROCEEDINGS{10940569,
  author={Le, Nam and Taherdoost, Hamed},
  booktitle={2025 International Conference on Pervasive Computational Technologies (ICPCT)}, 
