  month={Dec},}@INPROCEEDINGS{10847381,
  author={Butt, Usman Javed and Alraja, Mansour and Salih, Abdulrahman},
  booktitle={2024 IEEE 16th International Conference on Computational Intelligence and Communication Networks (CICN)}, 
  title={Mitigating Ransomware in Cyber-Physical Human Systems Through Game-Based Learning}, 
  year={2024},
  volume={},
  number={},
  pages={420-425},
  abstract={The use of information and communication technology has evolved tremendously in the last decade. The dependency on this technology is an integral part of any business operations to achieve their strategic goal, which means compromise to any of these components, can severely disrupt and jeopardise business activities and can also bring a bad name to their reputation. As technology is becoming more sophisticated, there is a rise in more organised cyber-attacks. Organisations struggle to develop an effective Cyber security strategy, mainly because more user awareness is required in the domain. Since the number of Internet users and the integration of technologies in our lives is increasing exponentially. Humans live in a cyber social system, in which they interact with smart infrastructure. Attackers exploit humans as the weakest link to execute successful security breaches and gain an advantage in the lucrative cybercrime market. There is a dire need to address new challenges posed by cybercriminals and mitigate risks to human digital lives.},
