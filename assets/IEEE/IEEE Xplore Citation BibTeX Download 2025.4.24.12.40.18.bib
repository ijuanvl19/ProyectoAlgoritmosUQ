  isbn={9788770227643},
  url={https://ieeexplore-ieee-org.crai.referencistas.com/document/9933664},}@INPROCEEDINGS{6932873,
  author={Liapis, Antonios and Yannakakis, Georgios N. and Togelius, Julian},
  booktitle={2014 IEEE Conference on Computational Intelligence and Games}, 
  title={Designer modeling for Sentient Sketchbook}, 
  year={2014},
  volume={},
  number={},
  pages={1-8},
  abstract={This paper documents the challenges in creating a computer-aided level design tool which incorporates computergenerated suggestions which appeal to the human user. Several steps are suggested in order to make the suggestions more appropriate to a specific user's overall style, current focus, and end-goals. Designer style is modeled via choice-based interactive evolution which adapts the impact of different dimensions of quality based on the designer's choice of certain suggestions over others. Modeling process is carried out similarly to style, but adapting to the current focus of the designer's actions. Goals are modeled by estimating the visual patterns of the designer's final artifact and changing the parameters of the algorithm to enforce such patterns on generated suggestions.},
  keywords={Adaptation models;Games;Computational modeling;Visualization;Sociology;Statistics;Load modeling},
  doi={10.1109/CIG.2014.6932873},
  ISSN={2325-4289},
  month={Aug},}@INPROCEEDINGS{9247773,
  author={Azam, Muhammad Hamza and Hasan, Mohd Hilmi and Hassan, Saima and Abdulkadir, Said Jadid},
  booktitle={2020 International Conference on Computational Intelligence (ICCI)}, 
  title={Fuzzy Type-1 Triangular Membership Function Approximation Using Fuzzy C-Means}, 
  year={2020},
  volume={},
  number={},
  pages={115-120},
  abstract={Fuzzy logic is a way of many-valued computing logic that deals with the truth values of the variables between 0 and 1, unlike the conventional Boolean logic. Membership functions are used to depict the fuzzy values of given variable. Though membership functions are determined through expert's opinion, however, the one estimated through heuristic algorithms is the preferable methods. Membership functions determined through statistical and knowledge engineering methods are usually application dependent and cannot be applied on different datasets. This research focuses on generating the parametric values of the triangular membership function using a novel method. Initially, the Fuzzy C-means algorithm is utilized to generate the parameters values of the Gaussian membership function. Using a set of equations, these values then estimate the parameters of the triangular membership function. The proposed method is applied to the quality of web services data. From the results it is verified that the new approach of generating triangular membership functions can be adopted.},
  keywords={Knowledge engineering;Fuzzy logic;Web services;Heuristic algorithms;Function approximation;Forecasting;Computational intelligence;Fuzzy Logics;Fuzzy-C Means;Gaussian MF;Membership Functions;Triangular MF.},
  doi={10.1109/ICCI51257.2020.9247773},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{5284275,
  author={Yilmaz, Levent},
  booktitle={2009 International Conference on Computational Science and Engineering}, 
  title={On the Synergy of Conflict and Collective Creativity in Open Innovation Socio-technical Ecologies}, 
  year={2009},
  volume={4},
  number={},
  pages={502-508},
  abstract={Sustaining open innovation requires better understanding how collaborative efforts of contributors yield collective creativity. One of the significant problems in understanding emergence of collective creativity involves how collectives govern and coordinate the actions of individuals to maximize innovation output. Yet, relatively little is known about desirable organizing processes in open innovation communities. To this end, we examine the impact of governance mechanisms and conflict management styles on collective creativity in open source socio-ecologies. Systems model of creativity and proxy metrics for measuring potential for collective creativity are utilized to conduct a simulation study of open source software socio-ecologies. Findings suggest that decentralized coordination schemes such as emergent selection that are observed in utility communities along with moderate degrees of assertiveness and cooperation for conflict management result in higher incidence of innovation.},
  keywords={Technological innovation;Environmental factors;Disaster management;Biological system modeling;Computational modeling;Innovation management;Stability;Testing;Project management;Decision making;agent simulation;open innovation;collective creativity;complex adaptive systems},
  doi={10.1109/CSE.2009.218},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{4219041,
  author={Mayer, Helmut A.},
  booktitle={2007 IEEE Symposium on Computational Intelligence and Games}, 
  title={Board Representations for Neural Go Players Learning by Temporal Difference}, 
  year={2007},
  volume={},
  number={},
  pages={183-188},
  abstract={The majority of work on artificial neural networks (ANNs) playing the game of Go focus on network architectures and training regimes to improve the quality of the neural player. A less investigated problem is the board representation conveying the information on the current state of the game to the network. Common approaches suggest a straight-forward encoding by assigning each point on the board to a single (or more) input neurons. However, these basic representations do not capture elementary structural relationships between stones (and points) being essential to the game. We compare three different board representations for self-learning ANNs on a 5 times 5 board employing temporal difference learning (TDL) with two types of move selection (during training). The strength of the trained networks is evaluated in games against three computer players of different quality. A tournament of the best neural players, addition of alpha-beta search, and a commented game of a neural player against the best computer player further explore the potential of the neural players and its respective board representations},
  keywords={Humans;Computer networks;Computational intelligence;Game theory;Artificial neural networks;Computer architecture;Encoding;Neurons;Neural networks;Intelligent networks;Game of Go;Artificial Neural Networks;Temporal Difference Learning;Board Representation},
  doi={10.1109/CIG.2007.368096},
  ISSN={2325-4289},
  month={April},}@INPROCEEDINGS{7020696,
  author={Doboli, Alex and Doboli, Simona},
  booktitle={2014 IEEE Symposium on Computational Intelligence, Cognitive Algorithms, Mind, and Brain (CCMB)}, 
  title={Iflows: A novel simulation model for predicting the effectiveness of a research community}, 
  year={2014},
  volume={},
  number={},
  pages={71-78},
  abstract={This paper presents a simulation model for observing the dynamics of a research community in engineering. The objective is to study how parameters, like group expertise and resources, influence the effectiveness of the group and community as a whole. The model implements a game-theoretic approach in which every group maximizes the difference between its rewards and costs (e.g., time and resources). Experiments studied the total reward and the number of problems solved over time by a community made of twenty groups for different conditions, i.e. allocated resources and group characteristics.},
  keywords={Communities;Integrated circuit modeling;Predictive models;Circuit synthesis;Computational modeling;Correlation;Technological innovation},
