@INPROCEEDINGS{1401215,
  author={Wang, Z. and Zhang, D.H. and He, W.P.},
  booktitle={2004 IEEE International Conference on Systems, Man and Cybernetics (IEEE Cat. No.04CH37583)}, 
  title={Using introspection and prototype driven to achieve design analysis in the process of concept generation based on information processing}, 
