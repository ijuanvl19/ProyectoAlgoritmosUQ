  year={2010},
  volume={},
  number={},
  pages={1-3},
  abstract={Williams Creativity Test B (WCTB) and Adolescent Scientific Creativity Scale (ASCS) were used to measure the creative affective and scientific creativity for 550 middle school students. Generalized regression neural network (GRNN) and multivariable linear regression (MLR) were used for modeling and testing. The result showed the fitting error of GRNN model was lower than the error of MLR. In the rough set analysis, the data was discretized with SOM network. After attribute reduction, eight rules were extracted.},
  keywords={Artificial neural networks;Psychology;Mathematical model;Data models;Training;Complexity theory;Linear regression},
  doi={10.1109/CISE.2010.5676983},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{10877594,
  author={Kant, Vishnu},
  booktitle={2024 Second International Conference Computational and Characterization Techniques in Engineering & Sciences (IC3TES)}, 
  title={Global Landmark Classification Using Enhanced ResNet50V2: A Deep Learning Approach}, 
  year={2024},
  volume={},
  number={},
  pages={1-5},
  abstract={Deep learning models have shown amazing success in picture categorization chores thanks to the fast developments in computer vision. Of them, convolutional neural networks (CNNs) have become a potent instrument for precisely identifying landmarks and objects. This work employs an upgraded ResNet50 architecture, ResNet50V2, to classify famous sites from all around the world. Twelve different sites total—Burj Khalifa, Chichen Itza, Christ the Redeemer, Eiffel Tower, Great Wall of China, Machu Picchu, Pyramids of Giza, Roman Colosseum, Statue of Liberty, Stonehenge, Taj Mahal, and Venezuela's Angel Falls. Renowned for their architectural and cultural value, these sites create a difficult classification challenge given their different structures, histories, and locations. For uses in tourism, cultural preservation, and instructional materials, accurate visual classification of these sites has great promise. ResNet50V2's deep residual learning architecture allows it to learn and generalize features from challenging datasets, so appropriate for this work. This work intends to assess ResNet50V2's effectiveness in differentiating these internationally known landmarks, therefore supporting the larger domains of picture classification and landmark identification. Furthermore, in line with world sustainability objectives, this study supports efforts toward sustainable cultural heritage management, promotes education, tourism, and digital preservation by precisely identifying and classifying these locations.},
  keywords={Deep learning;Visualization;Accuracy;Education;Computer architecture;Robustness;Cultural differences;Object recognition;Convolutional neural networks;Sustainable development;Artificial Intelligence;Deep learning;Image classification;Convolutional Neural Networks;ResNet50V2;Landmark recognition;Cultural heritage;Computer vision},
  doi={10.1109/IC3TES62412.2024.10877594},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{6257621,
  author={Maller, Patricio and Salamon, Alicia and Mira, Natalia and Boggio, Alejandra and Giró, Juan and Cuozzo, Jose and Coenda, Francisco and Perez, Sofia},
  booktitle={2012 12th International Conference on Computational Science and Its Applications}, 
  title={New Practices to Structure and Elicit Improvement Opportunities in Scientific Software Development Teams}, 
  year={2012},
  volume={},
  number={},
  pages={121-124},
  abstract={This paper presents ongoing research on a new approach to structure and elicit improvement opportunities in the software development process followed by tecnichal-scientific teams. The bottom-up approach has been proved as more effective in process improvement applied to technical and scientific environments, in oppocition to standard improvement models such as ISO or CMMI. However, when bottom-up is applied to elicit improvement areas, the method is usually limited to the use of surveys that miss many of the root causes and structural deficiencies. To complement the elicitation process, we propose the use of requirement-gathering techniques such as Card Sorting, Kelly Grid and Multi-Voting. Initial results are discussed in this paper.},
  keywords={Software;Programming;Sorting;Software engineering;Interviews;Scientific computing;Modeling;Kelly Grid;Card Sorting;Multi Voting;Survey;Scientist;Scientific Software},
  doi={10.1109/ICCSA.2012.30},
  ISSN={},
  month={June},}@INPROCEEDINGS{10131752,
  author={Mishra, Rajneesh},
  booktitle={2023 International Conference on Computational Intelligence and Knowledge Economy (ICCIKE)}, 
  title={Comparative Analysis of Quality Assurance Models for Business Excellence: with special reference to ISO 9001 and EFQM model}, 
  year={2023},
  volume={},
  number={},
  pages={450-454},
  abstract={Global business environment in the modern times is being impacted by various forces such as advancement in technology, borderless trade, economic tremors and disruptions of business which have resulted in organizations adopting and using business excellence practices in order to achieve success in all domains of their business. There are several business excellence models available which are very famous amongst businesses, government and private accreditation agencies, regulators and policy makers. ISO has remained an all-time favorite quality standard for businesses. In the journey of excellence, most of the companies start with getting them in line with ISO standards and get certification. EFQM has also become very popular amongst modern day businesses as it sets more comprehensive view of business excellence and quality assurance. The aim of this paper is to understand the significance of business excellence and its internationally recognized models such as EFQM and ISO which assist in improving business performance. The paper also intends to highlight the similarities and dissimilarities between the two models and their implementation. Also, this is an effort to compare the two models (ISO and EFQM) to see their relevance and impact in the area of quality assurance and business excellence.},
  keywords={Economics;Analytical models;Quality assurance;Regulators;Biological system modeling;ISO Standards;ISO;quality assurance;business excellence;ISO;EFQM model;European foundation for quality management},
  doi={10.1109/ICCIKE58312.2023.10131752},
