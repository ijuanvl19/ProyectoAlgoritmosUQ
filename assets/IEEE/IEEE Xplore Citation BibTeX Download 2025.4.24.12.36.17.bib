  pages={682-685},
  abstract={social media in Education has worked like a revolution in changing the learning system. Learning according to the convenience such as place and time of the students has pulled more interest to educate more through social media. In this study, we had investigated the perspective of students from different educational levels towards social media learning. A total no. of 130 students participated in our online survey. Convenient sampling was used for the study. According to the results and findings of the study learning through social media has both positive and negative impacts. The majority of respondents spend 1–3 hours a day on average. Additionally, we found that Youtube is the most used application by students for social media learning among Facebook, Instagram, Telegram, Whatsapp, and YouTube. Although learning through social media makes a great contribution for learners in their area of interest. However, it has also affected the physical and mental health of the students. Due to social media learning, socialization among people has been reduced.},
  keywords={Performance evaluation;Learning systems;Freeware;Social networking (online);Education;Multimedia Web sites;Mental health;Education;social media;Learning},
  doi={10.1109/ComPE53109.2021.9751858},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{10721948,
  author={Kalpana, Ponugoti and Kodati, Sarangam and Sreekanth, Nara and Ali, Hassan Mohamed and A C, Ramachandra},
  booktitle={2024 International Conference on Intelligent Algorithms for Computational Intelligence Systems (IACIS)}, 
  title={Predictive Analytics for Crime Prevention in Smart Cities Using Machine Learning}, 
  year={2024},
  volume={},
  number={},
  pages={1-4},
  abstract={Crime prevention in smart city infrastructure significantly impacts improving quality of human life. However, the substantial increase in the urban population in recent years affects accuracy, safety, and security. This paper presents a solution using the Hyperplane to improve the Radial Basis Function with Support Vector Machine (SVM) based on Machine Learning techniques to enhance accuracy, safety, and security. The SVM method aims to reduce crime rates through proactive measures based on predictive insights. The RBF kernel handles nonlinear relationships by mapping input features into a higher-dimensional space, allowing the model to capture complex patterns. Initially, data is obtained from the Chicago crime dataset and pre-processed by checking for missing values and performing feature scaling to ensure consistency. Feature selection is conducted using Principal Component Analysis (PCA) to identify relevant features from the data. Crime prediction using RBF with SVM is performed efficiently to enhance accuracy. The proposed RBF-SVM model is evaluated using the Chicago crime dataset, achieving a high accuracy of 0.89, a precision of 0.80, a recall of 0.82, and an F1-score of 0.85. The performance is compared to existing techniques such as Decision Tree (DT) and Long Short-Term Memory (LSTM).},
  keywords={Support vector machines;Accuracy;Smart cities;Prevention and mitigation;Feature extraction;Safety;Security;Kernel;Long short term memory;Principal component analysis;crime predication;chicago crime dataset;principal component analysis;radial basis function and support vector machine},
  doi={10.1109/IACIS61494.2024.10721948},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{8916695,
  author={Yokoyama, Yuya and Hochin, Teruhisa and Nomiya, Hiroki},
  booktitle={2019 6th International Conference on Computational Science/Intelligence and Applied Informatics (CSII)}, 
  title={Obtaining Factors of Q&A Statements with the Consideration of Categories}, 
  year={2019},
  volume={},
  number={},
  pages={19-24},
  abstract={In order to avoid the problem of mismatch between questioner and respondent, an impression evaluation experiment was conducted and nine factors were obtained as a result. The factors were then used to estimate the factor scores of all other statements through multiple regression analysis and to select the most appropriate respondents. The factors, however, have been obtained without taking into consideration the categories of the Q&A statements. In this paper we therefore investigated whether the consideration of categories could lead to obtaining more apparent tendencies and precise results to select the most appropriate respondents. In this paper, factor analysis is applied to the experimental results with a consideration of the following categories: Auction, PC, Love, and Politics. As a result of factor analysis, both similarity and difference could be observed for each category of Q&A statements. Comparing the new results with the previous one, two similar main factors could be obtained regardless of the category considered. On the other hand, factors other than the two have were also obtained for each respective category. This shows that the number of factors obtained for each category varies.},
  keywords={Eigenvalues and eigenfunctions;Estimation;Informatics;Regression analysis;Databases;Standards;Euclidean distance;Q&A Site;Factor Analysis;Category},
