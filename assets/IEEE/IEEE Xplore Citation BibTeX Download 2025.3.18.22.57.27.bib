  month={April},}@INPROCEEDINGS{10619845,
  author={Vàraljai, Mariann and Nagy, Enikő and Nagy, Bàlint and Balogh, Zoltàn and Molnàr, György},
  booktitle={2024 IEEE 18th International Symposium on Applied Computational Intelligence and Informatics (SACI)}, 
