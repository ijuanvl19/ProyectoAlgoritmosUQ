@INPROCEEDINGS{6060520,
  author={Okamoto, Masako and Fujioka, Takeshi and Kita, Hajime},
  booktitle={SICE Annual Conference 2011}, 
  title={Agent-based modeling/simulation for teaching science and computer in high school}, 
  year={2011},
  volume={},
  number={},
  pages={1221-1223},
  abstract={Modeling of a phenomenon plays a key role in scientific thinking, and therefore an important issue in teaching science. Formulation of a model depends on methods to manipulate it. So far, differential equations have played a major role in scientific modeling because of model analysis using mathematics. However, with progress of information technology, computer simulation gets a powerful method to handle models, and individual-based and more procedural model such as an agent-based model can be another alternative in modeling. This study discusses an agent-based model of flue infection developed for teaching science and computer in a project-based learning (PBL) course in a senior high school. Course practice showed that the agent-based approach was effective in learning of modeling and simulation by experience.},
  keywords={Mathematical model;Computational modeling;Educational institutions;Computers;Problem-solving;Equations;teaching of science;teaching of computer;agent-based modeling;simulation},
  doi={},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10788173,
  author={Huang, Yanli},
  booktitle={2024 International Conference on Computers, Information Processing and Advanced Education (CIPAE)}, 
  title={A Study on the Application of Computer Aided Ecolinguistic Learning Technology in English Intelligent Classroom System}, 
  year={2024},
  volume={},
  number={},
  pages={190-194},
  abstract={This paper aims to explore the use of advanced resource retrieval algorithms under the framework of ecolinguistics theory, especially the KL-divergence retrieval model, to optimize the resource distribution and personalized learning path design of the English wisdom classroom system. Firstly, this paper introduces the basic concepts of ecolinguistics, emphasizing the dynamic and interactive nature of ecolinguistics learning. A CAELL framework combining the principles of ecolinguistics is proposed. The framework promotes students' acquisition of ecological language in interaction with computers by simulating the natural ecological language environment. Then, this paper analyzes the working mechanism of KL-divergence retrieval model, which can quantify the differences between different resources so as to accurately match students' learning needs. The research team constructed a prototype English wisdom classroom system and conducted a one-semester experiment to verify the effectiveness of the proposed method. The experimental results showed that students adopting CAELL technology and KL-divergence retrieval model were significantly better than those students adopting traditional teaching model in terms of vocabulary mastery, grammar application and oral communication. In addition, through the data mining and analysis of the learning process, the study also reveals the regularity of students' learning behavior, which provides a basis for further optimizing personalized learning strategies. The application of the computer-aided ecolingual learning technology and learning resource retrieval model in the intelligent English classroom system not only improves the teaching effect, but also provides a new way of thinking and practice for the future development of intelligent education.},
  keywords={Computers;Vocabulary;Adaptation models;Biological system modeling;Computational modeling;Education;Semantics;Thesauri;Indexes;Resource management;Computer-aided ecological language learning;KL-divergence retrieval model;English wisdom class;ecolinguistics;personalized learning},
  doi={10.1109/CIPAE64326.2024.00039},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{9850687,
  author={Qian, Yue},
  booktitle={2022 International Conference on Inventive Computation Technologies (ICICT)}, 
  title={Two-dimensional Data Network Modeling of Computer Network Teaching and Innovation Ability Cultivation}, 
  year={2022},
  volume={},
  number={},
  pages={1126-1129},
  abstract={This paper studies the two-dimensional data network modeling of computer network teaching and innovation ability training. Computer network courses have important practical significance in teaching, and there are certain difficulties in the teaching process. In the current teaching process, it is important for computer network courses. The teaching effect still needs to be improved. For this, teachers should continue to innovate the teaching model, summarize the problems and shortcomings in practice, and then improve the overall work quality and efficiency. Cultivate students' new sense of cooperation and innovation, so that students can develop Internet-plus thinking, and cultivate more computer talents for the times. Based on this, this article will mainly discuss the two-dimensional cross-modeling of computer education and innovation ability under the network teaching mode.},
  keywords={Training;Technological innovation;Computational modeling;Education;Information age;Computer networks;Data models;Two-dimensional Data;Network Modeling;Computer Network Teaching;Innovation Ability Cultivation},
  doi={10.1109/ICICT54344.2022.9850687},
