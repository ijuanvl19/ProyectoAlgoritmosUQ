  month={June},}@INPROCEEDINGS{8924363,
  author={Red'ko, Vladimir G.},
  booktitle={2019 10th IEEE International Conference on Intelligent Data Acquisition and Advanced Computing Systems: Technology and Applications (IDAACS)}, 
  title={Modeling of Cognitive Evolution}, 
  year={2019},
  volume={1},
  number={},
  pages={54-59},
  abstract={The report describes a new scientific area of investigation, namely, modeling of cognitive evolution. Cognitive evolution is the evolution of cognitive abilities of biological organisms. The important result of cognitive evolution is human thinking, which is used in the scientific cognition of nature. Modeling of cognitive evolution is the study of cognitive evolution by means of the mathematical and computer models of cognitive abilities of living organisms of different evolutionary levels. Underlying problems, backgrounds, and the sketch program for modeling of cognitive evolution are characterized. The initial models, which were developed in accordance with the sketch program, are briefly described. Possible applications of models of autonomous agents are discussed.},
  keywords={Evolution (biology);Mathematical model;Biological system modeling;Animals;Computational modeling;Analytical models;modeling of cognitive evolution;autonomous cognitive agents;biologically inspired models},
  doi={10.1109/IDAACS.2019.8924363},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10487429,
  author={Svetleff, Zvetomira},
  booktitle={2023 Congress in Computer Science, Computer Engineering, & Applied Computing (CSCE)}, 
  title={Investigating Persuasive Behaviors Across Different Opinion Polarities: A Study of Video Messages}, 
  year={2023},
  volume={},
  number={},
  pages={1176-1180},
  abstract={Interpreting subjectively what makes a person persuasive in online videos is a complex and dynamic process that requires critical thinking and active engagement from the part of the observer. With the rapid expansion of online communication through social networking websites, more persuasive content is made available online in the form of video. The use of positive and/or negative opinions might affect how persuasive someone appears to be. Persuasiveness is at the core of everyday human-human interactions, such as defending a client in a court, seeking a patient's compliance with medical advice or convincing a customer to buy a product. This study is motivated by the argument that persuasive behavior changes with opinion polarity and whether a speaker is trying to persuade one in favor of or against something. Emotion plays a key role in persuasion both in verbal and non-verbal communication, therefore understanding the impact of opinion polarity (sentiment) on the persuasive messages becomes increasingly important. The study aims to explain the” emotion” behind the message as expressed in negative and positive opinions both persuasive and unpersuasive videos. The data used for the analysis is derived from Persuasive Opinion Multimedia (POM) dataset which is an online multimodal dataset with existing labels for persuasive and non-persuasive videos placed in two sentiment or opinion polarities (positive and negative). This research paper also introduced a new neuromarketing assessment method called NeuroMap's NeuroScoring tool, previously designed to evaluate persuasive messages through a neuromarketing perspective. The present study results align with the NeuroMap model's emphasis on the role of emotions in persuasion. The persuasion model suggests that emotions directly impact how people process and memorize information and how persuasive messages that evoke positive emotions are more likely to be effective. In the study, sentiment levels can be seen as indicators of emotional content conveyed in the video messages, and the positive relationship between sentiment levels and persuasiveness supports the view that emotional appeal plays a crucial role in influencing persuasion.},
  keywords={Visualization;Social networking (online);Computational modeling;Education;Neuromarketing;Multimedia computing;Streaming media;Persuasive communication;Sentiment analysis;Persuasive Opinion Multimedia (POM);Multimodal Communication},
  doi={10.1109/CSCE60160.2023.00197},
  ISSN={},
  month={July},}@INPROCEEDINGS{9752768,
  author={Zhang, Xiaoli and Li, Rendong and Song, Mei and Hao, Wenhui},
  booktitle={2021 3rd International Conference on Internet Technology and Educational Informization (ITEI)}, 
  title={Exploration and Practice of the Online and Offline Teaching Mode of Computer Practice Course under the Standard of “Golden Course”}, 
  year={2021},
  volume={},
  number={},
  pages={56-59},
  abstract={Under the background of “Golden Course” construction, in order to stimulate college students' interest in “Multimedia Courseware Production” and improve their skills in courseware production, the essay explores the teaching mode of computer practice courses by online and offline hybrid teaching. The task-driven teaching approach and practical exercise approach are adopted in the course. The course fully excavates the ideological and political elements from given task content and organically integrates knowledge, abilities and quality objectives. In order to cultivate students' comprehensive ability and advanced thinking to solve complex problems, the teaching objectives of the course are set with the characteristics of high level, innovation and challenge.},
  keywords={Courseware;Technological innovation;Computational modeling;Education;Production;Internet;Task analysis;Golden Course;online and offline hybrid teaching;curriculum ideology and politics},
  doi={10.1109/ITEI55021.2021.00022},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{5622223,
  author={Liu Jia and Guo Weimin and Wang Xiaoqian},
  booktitle={2010 International Conference on Computer Application and System Modeling (ICCASM 2010)}, 
  title={Study on renewal of urban historic site by computer-aided design —A case of renewal conceptual design in Taosha Alley}, 
  year={2010},
  volume={14},
  number={},
  pages={V14-643-V14-647},
  abstract={This paper discusses the problems appearing in the current renewal of urban historic site by computer-aided design from the cross perspectives of environmental design, architecture and computer science. Based on the discussion of the relationship between the research of renewal of urban historic site and computer-aided design, the paper focuses on how to use computer-aided design during two important stages which are noumenal form of historic site and comparison in the process of design and attempts to explore a systematic and scientific research method. The case studies on renewal conceptual design in Taosha Alley in Wuxi by the use of computer-aided design. Through the analysis of the case,this paper aims to provide a convenient, intuitive, complete, systematic design thinking and reference for similar studies.},
  keywords={Rendering (computer graphics);Computational modeling;Lasers;Prototypes;Transportation;computer-aided design;urban historic site;renewal},
  doi={10.1109/ICCASM.2010.5622223},
  ISSN={2161-9077},
  month={Oct},}@INPROCEEDINGS{4675365,
  author={Xiying Huang and Jingye Wang},
  booktitle={2008 Asia Simulation Conference - 7th International Conference on System Simulation and Scientific Computing}, 
  title={On the capability structure of professional simulationists for simulation science and technology}, 
  year={2008},
  volume={},
  number={},
  pages={258-262},
  abstract={The requirement for simulation science and technology (SST) professionals is expatiated and the capability structure of qualified professional simulationists is established. The structure is composed of five key factors including the ability of thinking, innovation, mastery of theories, researching and working. The ability elements is interpreted and described in detail. We emphasize that it is necessary to education institution to synchronize knowledge-based capability with non-knowledge-based capability. On current status of simulation education and training, we point out that establish SST discipline and implement SST education is the good way to develop standardized and high-quality professional simulationists.},
  keywords={Computational modeling;Biological system modeling;Medical simulation;Chemical technology;Predictive models;Analytical models;Virtual manufacturing;Cognition;Acceleration;Sustainable development},
  doi={10.1109/ASC-ICSC.2008.4675365},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{6340129,
  author={Höfig, Edzard},
  booktitle={2012 IEEE 36th Annual Computer Software and Applications Conference}, 
  title={Supporting trust via open information spaces}, 
  year={2012},
  volume={},
  number={},
  pages={87-88},
  abstract={It is argued that creating better system transparency leads to an increased understanding of a system, which gives users better indicators for placing their trust. This way of thinking is behind the author's interest for open systems. By opening up the source, the interfaces, the standardisation or the operational data, one gains the ability to better understand, and therefore to potentially trust, a system.},
  keywords={Internet;Open systems;Humans;Aerospace electronics;Computational modeling;Educational institutions;Computer science},
  doi={10.1109/COMPSAC.2012.106},
  ISSN={0730-3157},
  month={July},}@ARTICLE{6212381,
  author={Doshi, Prashant and Qu, Xia and Goodie, Adam S. and Young, Diana L.},
  journal={IEEE Transactions on Systems, Man, and Cybernetics - Part A: Systems and Humans}, 
  title={Modeling Human Recursive Reasoning Using Empirically Informed Interactive Partially Observable Markov Decision Processes}, 
  year={2012},
  volume={42},
  number={6},
  pages={1529-1542},
  abstract={Recursive reasoning of the form what do I think that you think that I think (and so on) arises often while acting in multiagent settings. Previously, multiple experiments studied the level of recursive reasoning generally displayed by humans while playing sequential general-sum and fixed-sum, two-player games. The results show that subjects experiencing a general-sum strategic game display first or second level of recursive thinking with the first level being more prominent. However, if the game is made simpler and more competitive with fixed-sum payoffs, subjects predominantly attributed first-level recursive thinking to opponents thereby acting using second level. In this article, we model the behavioral data obtained from the studies using the interactive partially observable Markov decision process, appropriately simplified and augmented with well-known models simulating human learning and decision. We experiment with data collected at different points in the study to learn the models parameters. Accuracy of the predictions by our models is evaluated by comparing them with the observed study data, and the significance of the fit is demonstrated by comparing the mean squared error of our model predictions with those of a random hypothesis. Accuracy of the predictions by the models suggest that these could be viable ways for computationally modeling strategic behavioral data in a general way. While we do not claim the cognitive plausibility of the models in the absence of more evidence, they represent promising steps toward understanding and computationally simulating strategic human behavior.},
  keywords={Games;Markov processes;Decision making;Data models;Computational modeling;Predictive models;Computational models;human decision making;recursive reasoning;strategic games},
  doi={10.1109/TSMCA.2012.2199484},
  ISSN={1558-2426},
  month={Nov},}@ARTICLE{9308975,
  author={Cao, Lei and Zhang, Huijun and Feng, Ling},
  journal={IEEE Transactions on Multimedia}, 
  title={Building and Using Personal Knowledge Graph to Improve Suicidal Ideation Detection on Social Media}, 
  year={2022},
  volume={24},
  number={},
  pages={87-102},
  abstract={A large number of individuals are suffering from suicidal ideation in the world. There are a number of causes behind why an individual might suffer from suicidal ideation. As the most popular platform for self-expression, emotion release, and personal interaction, individuals may exhibit a number of symptoms of suicidal ideation on social media. Nevertheless, challenges from both data and knowledge aspects remain as obstacles, constraining the social media-based detection performance. Data implicitness and sparsity make it difficult to discover the inner true intentions of individuals based on their posts. Inspired by psychological studies, we build and unify a high-level suicide-oriented knowledge graph with deep neural networks for suicidal ideation detection on social media. We further design a two-layered attention mechanism to explicitly reason and establish key risk factors to individual&#x0027;s suicidal ideation. The performance study on microblog and Reddit shows that: 1) with the constructed personal knowledge graph, the social media-based suicidal ideation detection can achieve over 93&#x0025; accuracy; and 2) among the six categories of personal factors, <i>post, personality,</i> and <i>experience</i> are the top-3 key indicators. Under these categories, <i>posted text</i>, <i>stress level</i>, <i>stress duration</i>, <i>posted image</i>, and <i>ruminant thinking</i> contribute to one&#x0027;s suicidal ideation detection.},
  keywords={Blogs;Social networking (online);Psychology;Stress;Statistics;Sociology;Knowledge engineering;Personal knowledge graph;social interaction;social media;suicidal ideation detection},
  doi={10.1109/TMM.2020.3046867},
  ISSN={1941-0077},
  month={},}@INPROCEEDINGS{7016962,
  author={Liu, Wenjuan and Hei, Xinhong and Fukuma, Shinji and Mori, Shin-Ichiro},
  booktitle={2014 Tenth International Conference on Computational Intelligence and Security}, 
  title={A Simple and Real-Time Parallel Compression of Time Series Scientific Simulation Data for Interactive and Cooperative Supercomputing}, 
  year={2014},
  volume={},
  number={},
  pages={578-582},
  abstract={In order to realize a cooperative supercomputing environment, Large-scale scientific simulation data should be exchanged among supercomputers over the internet. To maintain the interactivity of the time-series simulation in such an environment, real-time data compression is required. Given the accuracy requirements of scientific simulation, we only focus on lossless data compression. In this paper, we propose Think Mantissa as Integer method to decrease the entropy of scientific datasets, achieving a good compression ratio and speeds. We evaluate this scheme in two experiments and compare it with the existing compression strategies (gzip, bzip2). Our approach gets about a 30% improvement in the compression ratio and hundreds of times speed acceleration. We also computed the total time of the data transfer using this method under some specific bandwidth cases, and nearly all of them have gotten a good acceleration.},
  keywords={Data models;Computational modeling;Data compression;Time series analysis;Parallel processing;Computational efficiency;Fluids;Lossless compression;Parallel Processing;Real-time Interaction;Supercomputing},
  doi={10.1109/CIS.2014.107},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{5376291,
